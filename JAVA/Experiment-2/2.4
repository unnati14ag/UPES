import java.util.*;
public class pattern {
    public static void main(String[] args) {
        Scanner Sc = new Scanner(System.in);
        int n,i,j;
        System.out.println("Enter the value of n");
        n = Sc.nextInt();
        for(i=1;i<=n;i++)
        {
        	for(j=1;j<=i;j++)
        	{
        		System.out.print("*");
        	}
        	System.out.println("");
        }
    }
}
