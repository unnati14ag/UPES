package pack1;
public class Arithmetic
{
	public void add(int x, int y)
	{
		System.out.println("Sum of numbers = "+(x+y));
	}
	public void sub(int x , int y)
	{
		System.out.println("Difference of numbers = "+(x-y));
	}
	public void mul(int x , int y)
	{
		System.out.println("Product of numbers = "+(x*y));
	}
	public void div(int x , int y)
	{
		System.out.println("Division of numbers = "+(x/y));
	}
	public void mod(int x , int y)
	{
		System.out.println("Modulus of numbers = "+(x%y));
	}
}
